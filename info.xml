<?xml version="1.0"?>
<extension key="eu.businessandcode.disschedrem" type="module">
  <file>disschedrem</file>
  <name>Disable Schedule Reminders</name>
  <description>disables irrelevant schedules reminders</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/eu.businessandcode.disschedrem.git</url>
    <url desc="Documentation">https://github.com/AlainBenbassat/eu.businessandcode.disschedrem.git</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-13</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Run this API: System.disableafterstart</comments>
  <civix>
    <namespace>CRM/Disschedrem</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
